<h1>All Articles</h1>
<ul id="articles">
  <% @articles.each do |article|%>
    <li>
      <%= link_to article.title, article_path(article), class: 'article_title' %>
      <p>
        <%= article.body%>
      </p>
      <p> Created at: 
        <%= article.created_at %>
      </p>
    </li>
  <%end%>
</ul>
<br>
<h4>
<% if logged_in? %>
  <%= link_to "Create new blog post", new_article_path %>
<% end %>
  <%= link_to "Show all tags", tags_path %>
</h4>